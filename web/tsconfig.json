{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@client/*": [
        "./src/client/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@icons/*": [
        "./src/components/icons/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@stores/*": [
        "./src/stores/*"
      ],
      "@typedef/*": [
        "./src/typedef/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    }
  },
}
